<h2>Listar</h2>
<div class="row">
  {{#each model as |equipe|}}
    <div class="col-md-4">
      <div class="panel panel-default library-item">
        <div class="panel-heading">
          <h3 class="panel-title">{{equipe.nome}}</h3>
        </div>
        <div class="panel-body">
            <p>Facebook: {{equipe.facebook}}</p>
            <p>Instagram: {{equipe.instagram}}</p>
            <p>Função: {{equipe.funcao}}</p>
            <p>Rifle: {{equipe.rifle}}</p>
            <p>Imagem: {{equipe.imagem}}</p>
        </div>
        <div class="panel-footer text-right">
          {{#link-to 'equipe.edit' equipe.id class='btn btn-success btn-xs'}}Editar{{/link-to}}
          <button class="btn btn-danger btn-xs" {{action 'deleteEquipe' equipe}}>Excluir</button>
        </div>
      </div>
    </div>
  {{/each}}
</div>