<h2>Listar</h2>
<div class="row">
  {{#each model as |usuario|}}
    <div class="col-md-4">
      <div class="panel panel-default library-item">
        <div class="panel-heading">
          <h3 class="panel-title">{{usuario.nome}}</h3>
        </div>
        <div class="panel-body">
          <p>Login: {{usuario.login}}</p>
          <p>Senha: {{usuario.senha}}</p>
        </div>
        <div class="panel-footer text-right">
          {{#link-to 'usuario.edit' usuario.id class='btn btn-success btn-xs'}}Editar{{/link-to}}
          <button class="btn btn-danger btn-xs" {{action 'deleteUsuario' usuario}}>Excluir</button>
        </div>
      </div>
    </div>
  {{/each}}
</div>