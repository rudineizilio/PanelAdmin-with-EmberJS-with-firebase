<h2>Listar</h2>
<div class="row">
  {{#each model as |carrossel|}}
    <div class="col-md-4">
      <div class="panel panel-default library-item">
        <div class="panel-heading">
          <h3 class="panel-title">{{carrossel.nome}}</h3>
        </div>
        <div class="panel-body">
          <p>Link: {{carrossel.link}}</p>
          <p>Imagem: {{carrossel.imagem}}</p>
        </div>
        <div class="panel-footer text-right">
          {{#link-to 'carrossel.edit' carrossel.id class='btn btn-success btn-xs'}}Editar{{/link-to}}
          <button class="btn btn-danger btn-xs" {{action 'deleteCarrossel' carrossel}}>Excluir</button>
        </div>
      </div>
    </div>
  {{/each}}
</div>